{{#each this}}
<div class="card mb-2">
	<div class="card-header card-header-sm"><b>Remote Track {{track}}</b>
					<button aria-label="Add Contact" data-key="{{mysip peer}}" class="btn btn-success btn-sm addcontactshort {{contacthide}}" style="margin-top: -2px;">
						<span class="fa fa-user-plus" aria-hidden="true"></span>
					</button>
					<button aria-label="DTMF" data-key="{{@key}}" class="btn btn-primary btn-sm keyboard" style="margin-top: -2px;">
						<span class="fas fa-th" aria-hidden="true"></span>
					</button>
					<button aria-label="Channel Mix" data-key="{{@key}}" class="btn btn-primary btn-sm chmix" style="margin-top: -2px;">
                       {{chmix}}
					</button>
		<div class="float-right">
					<span class="badge badge-primary">{{state}}</span>
					<button aria-label="Cancel Call" data-key="{{@key}}" class="btn btn-danger btn-sm hangup" style="margin-top: -2px;">
						<i class="fas fa-phone-slash"></i>
					</button>
		</div>
	</div>
	<div class="card-body">
		ID: {{mysip peer}}
		(Buffer: <span id="jitter{{track}}"></span> ms)
		Talk: <span id="talk{{track}}"></span>
		<div id="headphonesbar{{ch}}" class="progress progress-sm"></div>
	</div>
</div>
{{/each}}
